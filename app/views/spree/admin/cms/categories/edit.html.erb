<% content_for :page_title do %>
  Edit Category
<% end %>

<% content_for :page_actions do %>
  <%= link_to_with_icon 'arrow-left', 'Back To Category List', admin_cms_categories_path, class: 'btn btn-default' %>
<% end %>

<%= render :partial => 'spree/admin/shared/error_messages', :locals => { :target => @category} %>

<%= form_for [:admin, :cms, @category] do |f| %>
  <fieldset>
    <%= render :partial => 'form', :locals => { :f => f } %>
    <%= render :partial => 'spree/admin/shared/edit_resource_links' %>
  </fieldset>
<% end %>
